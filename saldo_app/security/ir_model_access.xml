<!-- ## CREAR LOS REGISTRO DE ACCESO  PARA PODER VISULIZAR EL MODULO EN ODDO## -->
<odoo>
    <!-- permisos para movimientos -->
    <record id="model_access_movimiento" model="ir.model.access">
        <!-- nombre del permiso -->
        <field name="name">model.access.movimiento</field>
        <!-- LOS PUNTOS SE CONVIERTEN EN GUIN BAJO -->
         <!-- hace refeencia al grupo administracion /ajustes -->
        <field name="model_id" ref="saldo_app.model_sa_movimiento"/>
        <field name="group_id" ref="base.group_system"/>
        <field name="perm_read" eval="True" />
        <field name="perm_create" eval="True" />
        <field name="perm_write" eval="True" />
        <field name="perm_unlink" eval="True" />
    </record>
    

    <!-- permisos para Categorias -->
    <record id="model_access_categoria" model="ir.model.access">
        <!-- nombre del permiso -->
        <field name="name">model.access.categoria</field>
        <!-- LOS PUNTOS SE CONVIERTEN EN GUIN BAJO -->
        <field name="model_id" ref="saldo_app.model_sa_category"/>
        <!-- hace refeencia al grupo administracion /ajustes -->
        <field name="group_id" ref="base.group_system"/>
        <field name="perm_read" eval="True" />
        <field name="perm_create" eval="True" />
        <field name="perm_write" eval="True" />
        <field name="perm_unlink" eval="True" />
    </record>

    
   <!--  permisos para tags -->
    <record id="model_access_tags" model="ir.model.access">
        <!-- nombre del permiso -->
        <field name="name">model.access.tags</field>
        <!-- LOS PUNTOS SE CONVIERTEN EN GUIN BAJO -->
        <field name="model_id" ref="saldo_app.model_sa_tag"/>
        <field name="group_id" ref="base.group_system"/>
        <field name="perm_read" eval="True" />
        <field name="perm_create" eval="True" />
        <field name="perm_write" eval="True" />
        <field name="perm_unlink" eval="True" />
    </record>
</odoo>