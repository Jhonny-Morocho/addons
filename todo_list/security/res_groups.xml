<!-- poder visualizar la aplicacion en el menu de aplicaciones y dar los permisos para que puedan ver -->
<!-- <?xml version="1.0" encoding="utf-8"?> -->
<odoo>
    <record id="model_acceso_todo" model="ir.model.access">
        <field name="name">model.access.todo</field>
        <field name="model_id" ref="todo_list.model_todo_app"/>
        <field name="group_id" ref="base.group_system"/>
        <field name="perm_read"  eval="1"/>
        <field name="perm_create"  eval="1"/>
        <field name="perm_write"  eval="1"/>
        <field name="perm_unlink"  eval="1"/>
    </record>
</odoo>

 